{
  "configVersion": "v3",
  "app": {
    "appId": 20001,
    "appName": "Zepptchi",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.0"
    },
    "icon": "icon.png",
    "vender": "zepp",
    "description": "Zepptchi is an app designed to gamify your health goals! Take care of your virtual pet by moving."
  },
  "permissions": [
    "data:os.device.info",
    "device:os.local_storage",
    "data:user.hd.step"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "3.0.0",
      "target": "3.0.0",
      "minVersion": "3.0"
    }
  },
  "targets": {
    "gt": {
      "module": {
        "page": {
          "pages": ["page/gt/index", "page/gt/food-list"]
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        },
        {
          "st": "s",
          "dw": 390
        }
      ]
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Zepptchi"
    }
  },
  "defaultLanguage": "en-US"
}
